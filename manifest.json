{
  "manifest_version": 2,
  "name": "RightScale SSH",
  "version": "1.5.3",
  "description": "Provides an ssh:// protocol link for the SSH button on the RightScale control panel.",
  "content_scripts": [
    {
      "matches": ["https://*.rightscale.com/*"],
      "js": ["zepto.min.js", "rs.js"]
    }
  ],
  "icons": {
    "16": "icons/small.png",
    "48": "icons/medium.png",
    "128": "icons/large.png"
  }
}
